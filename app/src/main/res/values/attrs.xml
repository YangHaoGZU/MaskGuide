<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LinkView">
        <attr name="start_point_color" format="color"/>
        <attr name="start_point_radius" format="dimension"/>
        <attr name="end_point_color" format="color"/>
        <attr name="end_point_radius" format="dimension"/>
        <attr name="line_color" format="color"/>
        <attr name="line_width" format="dimension"/>
        <attr name="startX" format="float"/>
        <attr name="startY" format="float"/>
        <attr name="endX" format="float"/>
        <attr name="endY" format="float"/>
        <attr name="turn_type">
            <enum name="base_start_x" value="0"/>
            <enum name="base_end_x" value="1"/>
            <enum name="direct" value="2"/>
        </attr>
        <attr name="autoPathAnim" format="boolean"/>
        <attr name="pathAnimDur" format="integer"/>
    </declare-styleable>
</resources>